            <packageType name="DotnetTool" />
        </packageTypes>
    </metadata>
    <files>
        <file src=".\LICENSE" target="docs/LICENSE"/>
        <file src=".\README.md" target="docs/README.md"/>
        <file src=".\icons-puzzle.png" target="docs/icons-puzzle.png"/>
        <file src=".\PatchNotes.md" target="docs/PatchNotes.md"/>
        <file src="src\SdkGenerator\bin\Release\net7.0\*" target="tools\net7.0\any" />
